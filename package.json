{
  "name": "league-stats-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start:app start:server",
    "start:app": "BROWSER=none react-app-rewired start",
    "start:server": "ts-node-dev --respawn --transpileOnly -r esm -r dotenv/config src/server.ts --exec nodemon -r esm",
    "build": "react-app-rewired build",
    "test": "CI=true BROWSER=none react-app-rewired test && start-server-and-test start http://localhost:3000 cypress:ci",
    "test:standard": "CI=true react-app-rewired test",
    "test:ci": "start-server-and-test start http://localhost:3000 cypress:ci",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src/ --ext .ts,.tsx,.js,.jsx",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run --config video=false",
    "format": "prettier-eslint --eslint-config-path '.prettier-eslintrc-styles.js' --write"
  },
  "proxy": "http://localhost:3001",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test && lint-staged --allow-empty"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn format"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.0-rc4",
    "@testing-library/jest-dom": "^5.4.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "request": "^2.88.2",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.1.0",
    "typescript": "~3.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.3",
    "@types/request": "^2.48.4",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-plugin-styled-components": "^1.10.7",
    "customize-cra": "^0.9.1",
    "cypress": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "node-env-run": "^3.0.2",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "prettier-eslint-cli": "^5.0.0",
    "react-app-rewired": "^2.1.5",
    "start-server-and-test": "^1.10.11",
    "ts-jest": "^25.3.1",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
